{
  "id": "de.busch-jaeger.freeathome.example",
  "name": {
    "en": "Example for free@home addons"
  },
  "author": "Busch Jaeger",
  "type": "app",
  "version": "1.0.0",
  "beta": true,
  "description": {
    "en": "Example for free@home addons"
  },
  "url": {
    "en": "http://busch-jaeger.de"
  },
  "entryPoint": "build/main.js",
  "license": "MIT",
  "parameters": {
    "default": {
      "items": {
        "apiKey": {
          "name": "API Key",
          "type": "string"
        },
        "ignoreReachability": {
          "name": "Ignore reachability",
          "type": "boolean"
        },
        "port": {
          "name": "Port",
          "type": "number",
          "max": 65536,
          "min": 1024
        },
        "username": {
          "name": "Username",
          "type": "string"
        }
      },
      "name": "Settings"
    }
  }
}